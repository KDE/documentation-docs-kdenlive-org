msgid "Examples"
msgstr "Příklady"

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:76
msgid "Video Grid effect with 3 rows and 6 columns"
msgstr ""

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:81
msgid "rows = 0.105 ⇒  means (0.1 X 19) + 1 =  3 rows"
msgstr ""

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:83
msgid "columns = 0.263 ⇒ means (0.263 X 19) + 1 = 6  columns"
msgstr ""

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:85
msgid ""
"In order to calculate the fractional value for :guilabel:`Rows` and :"
"guilabel:`Columns` for a specific number of rows and/or columns use this "
"formula:"
msgstr ""

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:87
msgid "``Rows/Columns = (#_of_rows/columns - 1) * 0.05263``"
msgstr ""

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:90
msgid "#_of_rows = 3 ⇒ means (3 - 1) * 0.05263 = 0.105"
msgstr ""

#: ../../effects_and_compositions/video_effects/generate/video_grid.rst:92
msgid "#_of_columns = 6 ⇒ means (6 -1) * 0.05263 = 0.263"
msgstr ""
