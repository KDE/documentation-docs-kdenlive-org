msgstr ""

#: ../../project_and_asset_management/project_bin/clips.rst:61
msgid ""
"|view-list-details|\\ :guilabel:`Compact View` switches to compact view"
msgstr ""

#: ../../project_and_asset_management/project_bin/clips.rst:63
msgid ""
"|view-list-tree|\\ :guilabel:`Details View` switches to details view"
msgstr ""

