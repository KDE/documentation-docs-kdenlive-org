#: ../../user_interface/menu/timeline_menu/tracks.rst:11
msgid "Tracks"
msgstr "Faixas"

#: ../../user_interface/menu/timeline_menu/tracks.rst:16
msgid "Contents"
msgstr "Conteúdo"

#: ../../user_interface/menu/timeline_menu/tracks.rst:23
msgid ""
"The submenu for inserting/deleting tracks can also be displayed by right-"
"clicking the track title (anywhere except the track name)."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:28
msgid "Master effects"
msgstr "Efeitos-mestres"

#: ../../user_interface/menu/timeline_menu/tracks.rst:35
msgid ""
"Let user quickly apply audio or video effects to all tracks. This can be "
"useful if you want one or more effects to be applied throughout the video."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:37
msgid ""
"Click the :guilabel:`Master` button above track headers to see master effect "
"stack."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:41
msgid "Insert Track"
msgstr "Inserir uma Faixa"

#: ../../user_interface/menu/timeline_menu/tracks.rst:50
msgid ""
"Displays a dialog which lets you choose which type of track to insert and "
"where – before or after a specified existing track."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:55
msgid "Delete Track"
msgstr "Apagar a Faixa"

#: ../../user_interface/menu/timeline_menu/tracks.rst:64
msgid "Displays a dialog which lets you choose which track to delete."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:69
msgid "Select All in Current Track"
msgstr "Seleccionar Tudo na Faixa Actual"

#: ../../user_interface/menu/timeline_menu/tracks.rst:73
msgid "Selects all clips in the current track."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:77
msgid "Select All"
msgstr "Seleccionar Tudo"

#: ../../user_interface/menu/timeline_menu/tracks.rst:81
msgid "Selects all clips in all tracks."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:85
msgid "Deselect"
msgstr "Deseleccionar"

#: ../../user_interface/menu/timeline_menu/tracks.rst:89
msgid "Cancels the selection."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:93
msgid "Toggle Track Lock"
msgstr "Comutar o Bloqueio da Faixa"

#: ../../user_interface/menu/timeline_menu/tracks.rst:97
msgid "Disables or allows editing on the selected track."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:101
msgid "Toggle All Track Lock"
msgstr "Comutar Todos os Bloqueios de Faixas"

#: ../../user_interface/menu/timeline_menu/tracks.rst:105
msgid "Disables or allows editing on all tracks."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:109
msgid "Toggle Track Target"
msgstr "Comutar o Destino da Faixa"

#: ../../user_interface/menu/timeline_menu/tracks.rst:113
msgid ""
"Click to toggle track as target. Target tracks will receive the inserted "
"clips."
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:117
msgid "Toggle Track Аctive"
msgstr ""

#: ../../user_interface/menu/timeline_menu/tracks.rst:121
msgid ""
"Click to make track active/inactive. Active tracks will react to editing "
"operations."
msgstr ""
