#: ../../effects_and_compositions/video_effects/transform_distort_perspective/defish.rst:81
msgid "manual variable"
msgstr "zmienna ręczna"

#: ../../effects_and_compositions/video_effects/transform_distort_perspective/defish.rst:83
msgid "\"Manual aspect\""
msgstr "\"Kształt ręczny\""

#: ../../effects_and_compositions/video_effects/transform_distort_perspective/defish.rst:85
