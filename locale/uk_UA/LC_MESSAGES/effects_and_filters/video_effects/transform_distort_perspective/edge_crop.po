#: ../../effects_and_filters/video_effects/transform_distort_perspective/edge_crop.rst:68
msgid "Left"
msgstr "Ліворуч"

#: ../../effects_and_filters/video_effects/transform_distort_perspective/edge_crop.rst:70
msgid "Number of pixels to be removed from the left edge"
msgstr "Кількість пікселів, які слід вилучити з лівого краю"

#: ../../effects_and_filters/video_effects/transform_distort_perspective/edge_crop.rst:71
msgid "Bottom"
msgstr "Знизу"

#: ../../effects_and_filters/video_effects/transform_distort_perspective/edge_crop.rst:73
msgid "Number of pixels to be removed from the bottom edge"
msgstr "Кількість пікселів, які слід вилучити з нижнього краю"

#: ../../effects_and_filters/video_effects/transform_distort_perspective/edge_crop.rst:74
msgid "Right"
msgstr "Праворуч"

