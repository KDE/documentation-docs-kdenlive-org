#: ../../project_and_asset_management/project_bin/generators.rst:114
msgid "SMPTE color bars"
msgstr "SMPTE färgfält"

#: ../../project_and_asset_management/project_bin/generators.rst:115
msgid "Philips PM5544"
msgstr "Philips PM5544"

#: ../../project_and_asset_management/project_bin/generators.rst:116
msgid "FuBK"
msgstr "FuBK"

#: ../../project_and_asset_management/project_bin/generators.rst:117
msgid "Simplified FuBK"
msgstr "Förenklad FuBK"

#: ../../project_and_asset_management/project_bin/generators.rst:121
msgid "White Noise"
msgstr "Vitt brus"

#: ../../project_and_asset_management/project_bin/generators.rst:126
msgid ""
"This generates a video noise clip - like the \"snow\" on an out-of-tune "
"analogue TV including the audio white noise"
msgstr ""
"Det genererar ett videobrusklipp, som \"myrornas krig\" på en ojusterad "
"analog TV inklusive det vita ljudbruset"

