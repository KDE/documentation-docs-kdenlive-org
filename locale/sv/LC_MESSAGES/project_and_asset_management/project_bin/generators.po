msgstr "Genererar ett färgtestmönster av olika typer."

#: ../../project_and_asset_management/project_bin/generators.rst:104
msgid "Color Bars generator"
msgstr "Färgfältsgenerator"

#: ../../project_and_asset_management/project_bin/generators.rst:108
msgid ":guilabel:`Bar Type`:"
msgstr ":guilabel:`Fälttyp`:"

#: ../../project_and_asset_management/project_bin/generators.rst:110
msgid "PAL color bars"
msgstr "PAL färgfält"

#: ../../project_and_asset_management/project_bin/generators.rst:111
msgid "PAL color bars with red"
msgstr "PAL färgfält med röd"

#: ../../project_and_asset_management/project_bin/generators.rst:112
