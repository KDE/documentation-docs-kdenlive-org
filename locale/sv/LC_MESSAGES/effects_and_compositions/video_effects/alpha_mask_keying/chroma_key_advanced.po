msgstr "Bestämmer mängden vävning"

#: ../../effects_and_compositions/video_effects/alpha_mask_keying/chroma_key_advanced.rst:78
msgid "Operation"
msgstr "Operation"

#: ../../effects_and_compositions/video_effects/alpha_mask_keying/chroma_key_advanced.rst:80
msgid "Determines how the alpha channel is processed"
msgstr "Bestämmer hur alfakanalen behandlas"

#: ../../effects_and_compositions/video_effects/alpha_mask_keying/chroma_key_advanced.rst:81
