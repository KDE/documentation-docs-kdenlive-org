#: ../../effects_and_compositions/video_effects/alpha_mask_keying/alpha_operations.rst:124
msgid "Shrink soft"
msgstr "Mjuk förminskning"

#: ../../effects_and_compositions/video_effects/alpha_mask_keying/alpha_operations.rst:126
msgid "Grow hard"
msgstr "Hård förstoring"

#: ../../effects_and_compositions/video_effects/alpha_mask_keying/alpha_operations.rst:127
#: ../../effects_and_compositions/video_effects/alpha_mask_keying/alpha_operations.rst:129
