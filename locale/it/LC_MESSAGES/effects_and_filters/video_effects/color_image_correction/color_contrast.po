#: ../../effects_and_filters/video_effects/color_image_correction/color_contrast.rst:56
msgid "Red-Cyan Contrast"
msgstr "Contrasto rosso-ciano"

#: ../../effects_and_filters/video_effects/color_image_correction/color_contrast.rst:57
#: ../../effects_and_filters/video_effects/color_image_correction/color_contrast.rst:60
#: ../../effects_and_filters/video_effects/color_image_correction/color_contrast.rst:63
#: ../../effects_and_filters/video_effects/color_image_correction/color_contrast.rst:66
