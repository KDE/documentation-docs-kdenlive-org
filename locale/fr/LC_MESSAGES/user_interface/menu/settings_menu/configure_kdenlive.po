msgstr "\"0b33\", ATTRS{idProduct}==\"0030\", MODE=\"0444\""

#: ../../user_interface/menu/settings_menu/configure_kdenlive.rst:236
msgid "for Contour ShuttlePRO V2. Obtain the device file by a command"
msgstr ""
