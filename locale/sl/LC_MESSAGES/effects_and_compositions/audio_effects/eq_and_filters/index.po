msgid "Kdenlive Audio Effects - EQ and Filters"
msgstr "Zvočni učinki Kdenlive - Izenačevalnik in filtri"

#: ../../effects_and_compositions/audio_effects/eq_and_filters/index.rst:1
msgid ""
"KDE, Kdenlive, documentation, user manual, video editor, open source, audio "
"effects, eq, equalizer, filters"
msgstr ""
"KDE, Kdenlive, učinki, dokumentacija, uporabniški priročnik, montažni "
"program, program za montažo, program za videomontažo, video urejevalnik, "
