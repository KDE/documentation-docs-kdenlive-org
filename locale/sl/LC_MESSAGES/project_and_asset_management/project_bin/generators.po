msgid "PAL color bars"
msgstr "Barvni stolpci PAL"

#: ../../project_and_asset_management/project_bin/generators.rst:112
msgid "PAL color bars with red"
msgstr "Barvni stolpci PAL z rdečo"

#: ../../project_and_asset_management/project_bin/generators.rst:113
msgid "BBC color bars"
msgstr "Barvni stolpci BBC"

#: ../../project_and_asset_management/project_bin/generators.rst:114
msgid "EBU color bars"
msgstr "Barvni stolpci EBU"

#: ../../project_and_asset_management/project_bin/generators.rst:115
msgid "SMPTE color bars"
msgstr "Barvni stolpci SMPTE"

#: ../../project_and_asset_management/project_bin/generators.rst:116
msgid "Philips PM5544"
msgstr "Philips PM5544"

#: ../../project_and_asset_management/project_bin/generators.rst:117
msgid "FuBK"
